{
  "hosting": {
    "public": "build",
    "site": "animara-dapp-internal",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [ { 
      "source": "/**",
      "destination": "/index.html",
      "dynamicLinks": true
      }
    ]
  }
}
